@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix repo: <http://terminusdb.com/schema/repository#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terminus: <http://terminusdb.com/schema/system#> .
@prefix system: <http://terminusdb.com/schema/system#> .
@prefix layer: <http://terminusdb.com/schema/layer#> .
@prefix woql: <http://terminusdb.com/schema/woql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix xdd: <http://terminusdb.com/schema/xdd#> .
@prefix vio: <http://terminusdb.com/schema/vio#> .
@prefix ref: <http://terminusdb.com/schema/ref#> .
@prefix api: <http://terminusdb.com/schema/api#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix scm: <terminusdb:///schema#> .
@prefix doc: <terminusdb:///data/> .

scm:Card
  a owl:Class ;
  rdfs:label "Card"@en .

scm:Concept
  a owl:Class ;
  rdfs:label "Concept"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity .

scm:Entity
  a owl:Class ;
  rdfs:label "Entity"@en ;
  rdfs:subClassOf terminus:Document .

scm:Event
  a owl:Class ;
  rdfs:label "Event"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity .

scm:File
  a owl:Class ;
  rdfs:label "File"@en ;
  rdfs:subClassOf terminus:Document .

scm:GND
  a owl:DatatypeProperty ;
  rdfs:domain scm:Person ;
  rdfs:label "GND"@en ;
  rdfs:range xdd:url .

scm:Image
  a owl:Class ;
  rdfs:label "Image"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:File .

scm:Material
  a owl:Class ;
  rdfs:label "Material"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Concept .

scm:Object
  a owl:Class ;
  rdfs:label "Object"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Concept .

scm:Person
  a owl:Class ;
  rdfs:label "Person"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity ;
  scm:wd "wd:Q5"@en .

scm:Place
  a owl:Class ;
  rdfs:label "Place"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity ;
  scm:wd "wd:Q2221906"@en .

scm:Production
  a owl:Class ;
  rdfs:label "Production"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Event .

scm:Ressource
  a owl:Class ;
  rdfs:label "Ressource"@en ;
  rdfs:subClassOf terminus:Document ,
                  scm:Concept .

scm:View
  a owl:Class ;
  rdfs:label "View"@en ;
  rdfs:subClassOf terminus:Document .

scm:broader
  a owl:ObjectProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "broader"@en ;
  rdfs:range scm:Entity .

scm:cards
  a owl:ObjectProperty ;
  rdfs:domain scm:View ;
  rdfs:label "cards"@en ;
  rdfs:range scm:Card .

scm:collapsed
  a owl:DatatypeProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "collapsed"@en ;
  rdfs:range xsd:boolean .

scm:entity
  a owl:ObjectProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "entity"@en ;
  rdfs:range scm:Entity .

scm:image
  a owl:ObjectProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "image"@en ;
  rdfs:range scm:Image .

scm:inputs
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "inputs"@en ;
  rdfs:range scm:Concept .

scm:outputs
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "outputs"@en ;
  rdfs:range scm:Concept .

scm:placeOfBirth
  a owl:ObjectProperty ;
  rdfs:domain scm:Person ;
  rdfs:label "place of birth"@en ;
  rdfs:range scm:Place ;
  scm:wdt "wdt:P19"@en .

scm:placeOfDeath
  a owl:ObjectProperty ;
  rdfs:domain scm:Person ;
  rdfs:label "place of death"@en ;
  rdfs:range scm:Place ;
  scm:wdt "wdt:P20"@en .

scm:takesPlaceIn
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "takes place in"@en ;
  rdfs:range scm:Place .

scm:url
  a owl:DatatypeProperty ;
  rdfs:domain scm:File ;
  rdfs:label "url"@en ;
  rdfs:range xdd:url .

scm:uses
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "uses"@en ;
  rdfs:range scm:Object .

scm:wd
  a owl:DatatypeProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "Wikidata Item"@en ;
  rdfs:range xsd:string .

scm:wdt
  a owl:DatatypeProperty ;
  rdfs:label "Wikidata Property"@en ;
  rdfs:range xsd:string .

scm:x
  a owl:DatatypeProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "x"@en ;
  rdfs:range xsd:decimal .

scm:y
  a owl:DatatypeProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "y"@en ;
  rdfs:range xsd:decimal .
