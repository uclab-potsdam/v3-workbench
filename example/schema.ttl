@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix repo: <http://terminusdb.com/schema/repository#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terminus: <http://terminusdb.com/schema/system#> .
@prefix system: <http://terminusdb.com/schema/system#> .
@prefix layer: <http://terminusdb.com/schema/layer#> .
@prefix woql: <http://terminusdb.com/schema/woql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix xdd: <http://terminusdb.com/schema/xdd#> .
@prefix vio: <http://terminusdb.com/schema/vio#> .
@prefix ref: <http://terminusdb.com/schema/ref#> .
@prefix api: <http://terminusdb.com/schema/api#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix scm: <terminusdb:///schema#> .
@prefix doc: <terminusdb:///data/> .

scm:Card
  a owl:Class ;
  rdfs:label "Card" .

scm:Concept
  a owl:Class ;
  rdfs:label "Concept" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity .

scm:Entity
  a owl:Class ;
  rdfs:label "Entity" ;
  rdfs:subClassOf terminus:Document .

scm:Event
  a owl:Class ;
  rdfs:label "Event" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity .

scm:File
  a owl:Class ;
  rdfs:label "File" ;
  rdfs:subClassOf terminus:Document .

scm:GND
  a owl:DatatypeProperty ;
  rdfs:domain scm:Person ;
  rdfs:label "GND" ;
  rdfs:range xdd:url .

scm:Image
  a owl:Class ;
  rdfs:label "Image" ;
  rdfs:subClassOf terminus:Document ,
                  scm:File .

scm:Material
  a owl:Class ;
  rdfs:label "Material" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Concept .

scm:Object
  a owl:Class ;
  rdfs:label "Object" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Concept .

scm:Person
  a owl:Class ;
  rdfs:label "Person" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity ;
  scm:background "teal" ;
  scm:light false ;
  scm:text "red" ;
  scm:wd "wd:Q5" .

scm:Place
  a owl:Class ;
  rdfs:label "Place" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Entity ;
  scm:background "blue" ;
  scm:light true ;
  scm:text "magenta" ;
  scm:wd "wd:Q2221906" .

scm:Production
  a owl:Class ;
  rdfs:label "Production" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Event .

scm:Ressource
  a owl:Class ;
  rdfs:label "Ressource" ;
  rdfs:subClassOf terminus:Document ,
                  scm:Concept .

scm:View
  a owl:Class ;
  rdfs:label "View" ;
  rdfs:subClassOf terminus:Document .

scm:background
  a owl:DatatypeProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "Card background color" ;
  rdfs:range xsd:string .

scm:broader
  a owl:ObjectProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "broader" ;
  rdfs:range scm:Entity .

scm:cards
  a owl:ObjectProperty ;
  rdfs:domain scm:View ;
  rdfs:label "cards" ;
  rdfs:range scm:Card .

scm:collapsed
  a owl:DatatypeProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "collapsed" ;
  rdfs:range xsd:boolean .

scm:entity
  a owl:ObjectProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "entity" ;
  rdfs:range scm:Entity .

scm:image
  a owl:ObjectProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "image" ;
  rdfs:range scm:Image .

scm:inputs
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "inputs" ;
  rdfs:range scm:Concept .

scm:light
  a owl:DatatypeProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "Card theme" ;
  rdfs:range xsd:boolean .

scm:outputs
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "outputs" ;
  rdfs:range scm:Concept .

scm:placeOfBirth
  a owl:ObjectProperty ;
  rdfs:domain scm:Person ;
  rdfs:label "place of birth" ;
  rdfs:range scm:Place ;
  scm:wdt "wdt:P19" .

scm:placeOfDeath
  a owl:ObjectProperty ;
  rdfs:domain scm:Person ;
  rdfs:label "place of death" ;
  rdfs:range scm:Place ;
  scm:wdt "wdt:P20" .

scm:takesPlaceIn
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "takes place in" ;
  rdfs:range scm:Place .

scm:text
  a owl:DatatypeProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "Card text color" ;
  rdfs:range xsd:string .

scm:url
  a owl:DatatypeProperty ;
  rdfs:domain scm:File ;
  rdfs:label "url" ;
  rdfs:range xdd:url .

scm:uses
  a owl:ObjectProperty ;
  rdfs:domain scm:Production ;
  rdfs:label "uses" ;
  rdfs:range scm:Object .

scm:wd
  a owl:DatatypeProperty ;
  rdfs:domain scm:Entity ;
  rdfs:label "Wikidata Item" ;
  rdfs:range xsd:string .

scm:wdt
  a owl:DatatypeProperty ;
  rdfs:label "Wikidata Property" ;
  rdfs:range xsd:string .

scm:x
  a owl:DatatypeProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "x" ;
  rdfs:range xsd:decimal .

scm:y
  a owl:DatatypeProperty ;
  rdfs:domain scm:Card ;
  rdfs:label "y" ;
  rdfs:range xsd:decimal .
